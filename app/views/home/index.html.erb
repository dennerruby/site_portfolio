 <!-- Masthead-->
        <header class="masthead">
            <div class="container h-100">
                <div class="row h-100 align-items-center justify-content-center text-center">
                    <div class="col-lg-10 align-self-end">
                        <h1 class="text-uppercase text-white font-weight-bold">Seja bem vindo ao meu site !</h1>
                        <hr class="divider my-4" />
                    </div>
                    <div class="col-lg-8 align-self-baseline">
                        <p class="text-white-75 font-weight-light mb-5"></p>
                        <a class="btn btn-primary btn-xl js-scroll-trigger" href="#about">Descubra mais</a>
                    </div>
                </div>
            </div>
        </header>
        <!-- About section-->
        <section class="page-section bg-primary" id="about">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-8 text-center">
                        <h2 class="text-white mt-0">Um pouco sobre min!</h2>
                        <hr class="divider light my-4" />
                        <p class="text-white-50 mb-4">Me chamo Denner viana sou programador full stack Ruby on Rails nivel junior. Amo tecnologia e mais ainda programação.Quer saber mais um pouco sobre min baixe meu curriculo no botão abaixo.</p>
                        <a class="btn btn-light btn-xl js-scroll-trigger" href="https://drive.google.com/open?id=19iTZaclhb3kS9-371kZNgmN2sdp8-95c"target="_blank">BAIXAR</a>
                    </div>
                </div>
            </div>
        </section>
        <!-- skills section-->
        <section class="page-section" id="services">
            <div class="container">
                <h2 class="text-center mt-0">Skills</h2>
                <hr class="divider my-4" />
                <div class="row">
                    <div class="col-lg-3 col-md-6 text-center">
                        <div class="mt-5">
                            <i class="fab fa-4x fa-ubuntu text-primary mb-4"></i>
                            <h3 class="h4 mb-2">Linux</h3>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 text-center">
                        <div class="mt-5">
                            <i class="fas fa-4x fa-laptop-code text-primary mb-4"></i>
                            <h3 class="h4 mb-2">Ruby On Rails</h3>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 text-center">
                        <div class="mt-5">
                            <i class="fab fa-4x fa-html5 text-primary mb-4"></i>
                            <h3 class="h4 mb-2">HTML</h3>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 text-center">
                        <div class="mt-5">
                            <i class="fab fa-4x fa-git-square text-primary mb-4"></i>
                            <h3 class="h4 mb-2">Git</h3>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- Portfolio section-->
        <section id="portfolio">
            <div class="container-fluid p-0"> 
                <div class="row no-gutters">
                    <div class="col-lg-4 col-sm-6"> 
                        <a <%= image_tag "portfolio/fullsize/1", class:"portfolio-box"%>
                            <%= image_tag 'portfolio/thumbnails/1',class:"img-fluid",alt:""%> 
                          <div class="portfolio-box-caption">
                                <div class="project-name">One Bitboot</div>
                            </div></a
                        >
                    </div> 
                    <div class="col-lg-4 col-sm-6">
                        <a <%= image_tag 'portfolio/fullsize/2',class:"portfolio-box"%>
                            <%= image_tag 'portfolio/thumbnails/2',class:"img-fluid",alt:""%> 
                            <div class="portfolio-box-caption">
                                <div class="project-name">RH Challenge</div>
                            </div></a
                        >
                    </div>
                    <div class="col-lg-4 col-sm-6">
                        <a <%= image_tag 'portfolio/fullsize/3',class:"portfolio-box"%>
                            <%= image_tag 'portfolio/thumbnails/3',class:"img-fluid",alt:""%> 
                            <div class="portfolio-box-caption">
                                <div class="project-name">Project Name</div>
                            </div></a
                        >
                    </div>
                </div>
            </div>
        </section>
        <!-- Call to action section-->
        <section class="page-section bg-dark text-white">
            <div class="container text-center">
                <h2 class="mb-4">Repositorio no Github</h2>
                <a class="btn btn-light btn-mini js-scroll-trigger"href="https://github.com/dennerruby" target="_blank"><i class="fab fa-4x fab fa-github text-primary mb-4"></i></a>
            </div>
        </section>
        <!-- Contact section-->
        <section class="page-section" id="contact">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-8 text-center">
                        <h2 class="mt-0">CONTATO</h2>
                        <hr class="divider my-4" />
                        <p class="text-muted mb-5"></p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4 ml-auto text-center mb-5 mb-lg-0">
                        <i class="fas fa-phone fa-3x mb-3 text-muted"></i>
                        <div>+1 (555) 123-4567</div>
                    </div>
                    <div class="col-lg-4 mr-auto text-center">
                        <i class="fas fa-envelope fa-3x mb-3 text-muted"></i
